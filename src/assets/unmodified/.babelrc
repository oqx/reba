{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "babel-plugin-react-scoped-css",
            {
                "include": ".local.(sa|sc|c)ss$"
            }
        ],
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "@babel/proposal-optional-chaining"
    ]
}